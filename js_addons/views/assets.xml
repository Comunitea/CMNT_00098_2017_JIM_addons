<?xml version="1.0"?>
<odoo>

    <template id="js_addons.backend" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/less" href="/js_addons/static/less/backend.less" />
        </xpath>

        <xpath expr="." position="inside">
            <script type="text/javascript" src="/js_addons/static/js/jquery.table-header-fixed.js"></script>
            <script type="text/javascript" src="/js_addons/static/js/functions.js"></script>
        </xpath>
    </template>

    <template id="js_addons.telesale" inherit_id="telesale.assets">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/js_addons/static/js/telesale.js"></script>
        </xpath>
    </template>

</odoo>