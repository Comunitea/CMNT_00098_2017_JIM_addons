<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="export_prices_cron" model="ir.cron">
            <field name='name'>Exportar todos los precios</field>
            <field name='interval_number'>1</field>
            <field name='interval_type'>days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="model">export.prices</field>
            <field name="function">compute_export_prices</field>
             <field eval="'(True,)'" name="args"/>
        </record>

        <record id="config_last_call_export_prices" model="ir.config_parameter">
            <field name="key">last_call_export_prices</field>
            <field name="value">2000-01-01 00:00:00</field>
        </record>
    </data>
</odoo>
