            <field name="name">wzd.product.report</field>
            <field name="model">wzd.product.report</field>
            <field name="arch" type="xml">
                <form string="Exportar a excell listado de productos">
                    <group>
                        <group>
                            <button name="product_web_to_xls" string="Productos web a XLS" type="object" class="oe_highlight"/>
                        </group>
                        <group>

                        </group>
                    </group>
                    <footer>
                        <button name="cancel_all" string="Cancelar" type="object" class="oe_highlight"/>
