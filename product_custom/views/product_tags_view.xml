<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="product_tag_form_add_web" model="ir.ui.view">
        <field name="name">product.tag.form.add.web</field>
        <field name="model">product.tag</field>
        <field name="inherit_id" ref="product_tags.product_tag_form"/>
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="web"/>
            </field>
        </field>
    </record>

    <record id="product_tag_tree_add_web" model="ir.ui.view">
        <field name="name">product.tag.list</field>
        <field name="model">product.tag</field>
        <field name="inherit_id" ref="product_tags.product_tag_tree"/>
        <field name="arch" type="xml">
            <field name="display_name" position="after">
                <field name="web"/>
            </field>
        </field>
    </record>

</odoo>
