<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="parameterization_select_variant_wizard_view" model="ir.ui.view">
        <field name="model">gotoproduct.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="product_variant" options="{'no_open': True, 'no_create': True}"/>
                </group>
                <footer>
                    <button name="go_to_variant" string="Switch" class="btn-primary" type="object"/> 
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
