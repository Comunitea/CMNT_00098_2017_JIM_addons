<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_move_tree_recheck_availabitliy" model="ir.ui.view">
            <field name="name">stock.move.tree.recheck.availability</field>
            <field name="model">stock.move</field>
            <field eval="10" name="priority"/>
            <field name="arch" type="xml">
                <tree string="Moves">
                    <field name="product_id"/>
                    <field name="product_uom_qty"/>
                    <field name="product_uom" options="{'no_open': True, 'no_create': True}" string="Unit of Measure" groups="product.group_uom"/>
                    <field name="state"/>
                    <button name="move_recheck_availability" string="Force recheck availability" type="object" icon="fa-refresh"
                            groups="stock_disable_force_availability_button.group_stock_force_availability"
                            states="confirmed,assigned,waiting,partially_available"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>